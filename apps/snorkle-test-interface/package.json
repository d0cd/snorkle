{
    "name": "aleo-website",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "webpack --config webpack.config.js",
        "build:vite": "vite build",
        "dev:webpack": "webpack-dev-server",
        "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0 && prettier . --check",
        "pretty": "prettier . --write",
        "preview": "vite preview",
        "deploy": "gh-pages -d dist",
        "start": "yarn build:vite && yarn preview",
        "start:dev": "vite --host",
        "clean": "rm -rf dist node_modules",
        "setup": "yarn clean && yarn install",
        "prod": "yarn setup && yarn start"
    },
    "dependencies": {
